class Solution {
    public boolean isValid(String s) {
        for (int i = 0; i < s.length(); i++) {
            if (s.charAt(i) == '(' ){
                if (s.charAt(i + 1) == ')')
                    return true;
                else
                    return false;
            }

            if (s.charAt(i) == '{') {
                if (s.charAt(i + 1) == '}')
                    return true;
                else
                    return false;
            }

            if (s.charAt(i) == '[') {
                if (s.charAt(i + 1) == ']')
                    return true;
                else
                    return false;
            }
        }
        return false;
    }
}
